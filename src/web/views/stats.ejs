<head>
  <title>Stats - Discord bot FR</title>
  <% include ./partials/head %>
  <link rel="stylesheet" type="text/css" href="/css/default/stats.css">
  <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet">
  <script type="text/javascript" src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
</head>
<body>
  <% include ./partials/header %>
  <div class="<%= theme == 1 ? 'presentation-dark' : 'presentation-light'%>" style="height: 100%;    margin-top: -10px;">
    <div style="background-image: url(../../img/patternmid.png); height: 100%">
      <div class="container">
        <div class="chart" data-percent="<%= ram %>">Ram <%= ram %> %</div>
        <div class="chart" data-percent="<%= cpuuse1 %>">CPU1 <%= cpuuse1 %>%</div>
        <div class="chart" data-percent="<%= cpuuse2 %>">CPU2 <%= cpuuse2 %>%</div>
      </div>
      <div class="container">
        <div id="app">
          <div class="row">
            <div class="col-xs-3">
              <div class="well">
                <h1 class="dash-red <%= theme == 1 ? 'dark' : 'light'%>">{{ activeUsers }} <i class="fa fa-user"></i></h1>
                <h3 class="text-muted">Utilisateurs Actif</h3>
              </div>
            </div>
            <div class="col-xs-9">
              <h2 class="sub-header">Pages Active</h2>
              <div class="table-responsive">
                <table class="table <%= theme == 1 ? 'dark' : 'light'%>">
                  <thead>
                    <td>Page URL</td>
                    <td>Utilisateurs Actif</td>
                  </thead>
                  <tbody>
                    <tr v-for="(page, count) in pages">
                      <td>{{ page }}</td>
                      <td>{{ count }}</td>
                    </tr>
                  </tbody>
                </table>
              </div>
              <h2 class="sub-header <%= theme == 1 ? 'dark' : 'light'%>">Référence</h2>
              <div class="table-responsive">
                <table class="table <%= theme == 1 ? 'dark' : 'light'%>">
                  <thead>
                    <td>Site référent</td>
                    <td>Utilisateurs Actif</td>
                  </thead>
                  <tbody>
                    <tr v-for="(referringSite, count) in referrers">
                      <td>{{ referringSite }}</td>
                      <td>{{ count }}</td>
                    </tr>
                  </tbody>
                </table>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</body>
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/mustache.js/2.3.0/mustache.min.js"></script>
<footer class="body">
